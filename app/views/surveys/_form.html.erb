<%= form_for @survey do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <%= f.fields_for :questions do |builder| %>
    <%= render "question_fields", :f => builder %>
  <% end %>
  <%= f.text_field :user_id, value: current_user.id,type: :hidden %>
  <p><%= f.submit "Submit" %></p>
<% end %>
